{
  "name": "genesys",
  "productName": "GeneSys",
  "version": "1.0.0",
  "description": "GeneSys - desktop app for CPU monitor and specs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"GeneSys\"",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Mircea Sima",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.1.2",
    "electron-packager": "^15.0.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "electron-log": "^4.2.2",
    "node-os-utils": "^1.3.0"
  }
}
